<h1> New Review </h1>

<%= form_for([@game, @game.reviews.build]) do |f| %>
<div id="rating-form" >
  <label> Rating: </label>
</div>
 

   <%= f.label :comment%>
  <%= f.text_field :comment%>

  <%= f.button :submit %>

<% end %>


<script>
    $('#rating-form').raty({
        path '/assets/',
        scoreName: 'review[rating]'
    });
   
</script>

